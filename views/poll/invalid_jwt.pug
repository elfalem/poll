extends poll_dialog_error
block content
  div.mdl-grid
    div.mdl-cell.mdl-cell--12-col.oops Oops!
  div.mdl-grid
    div.mdl-cell.mdl-cell--12-col Invalid or expired JWT.
  script.
    if (typeof AP !== 'undefined') {
      AP.require("dialog", function(dialog) {
        dialog.updatePrimaryAction({
        enabled: false
        })
      });
    }